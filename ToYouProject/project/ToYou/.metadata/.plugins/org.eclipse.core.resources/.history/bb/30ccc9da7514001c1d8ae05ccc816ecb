package com.toyou.project.service.user;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.toyou.project.dao.user.UserDAO;
import com.toyou.project.handler.GlobalExceptionHandler;
import com.toyou.project.model.UserVO;

@Service
public class UserServiceImpl implements UserService {

	@Autowired
	private UserDAO userDAO;
	
	
	@Override
	public int userJoin(UserVO vo) throws GlobalExceptionHandler{
		try {
			userDAO.userJoin(vo);
			return 1;
		}
		
	}

}
