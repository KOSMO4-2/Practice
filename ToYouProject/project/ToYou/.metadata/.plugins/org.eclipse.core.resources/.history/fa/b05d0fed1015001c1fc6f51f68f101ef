let index= {
		init:function(){
			$("#btn-save").on("click",()=>{
				this.save();
			});
		},
		
		save:function(){
			alert("회원가입 함수 실행")
			let data={
					user_id : $("#user_id").val(),
					user_passwrod:$("#user_password").val(),
					user_name:$("#user_name").val(),
					user_email:$("#user_email").val()	
			};
			console.log(data)
			
			
			$.ajax({
				type:"post",
				url:"/auth/joinProc",
				data:JSON.stringify(data),
				contentType:"application/json; charset=utf-8",
				dataType:"json"
			}).done(function(resp){
				alert("회원가입 완료");
				location.href="/";
			}).fail(function(error){
				console.log(error)
				alert(JSON.stringify(error));
			});
		}
}
index.init()